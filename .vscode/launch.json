            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "CMake: Build All"
        },
        {
            "name": "Debug CppApplication (CodeLLDB)",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/CppApplication/CppApplication",
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "DYLD_LIBRARY_PATH": "${workspaceFolder}/Library/ONNXRuntime/mac/lib:${workspaceFolder}/build/CppLibrary"
            },
            "preLaunchTask": "CMake: Build All"
        }
    ]
}
