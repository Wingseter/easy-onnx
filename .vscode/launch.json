{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CppApplication",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/CppApplication/CppApplication",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "DYLD_LIBRARY_PATH",
